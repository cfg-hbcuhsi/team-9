<link rel="stylesheet" href="/stylesheets/course-details.css">


  <header class="bg-light">
    <div class="container">
      <h1 class=" header-text">{{course.name}}</h1>
    </div>
  </header>


<div class="content-wrapper">
  <div class="circular-container"> 
    <a  id="clock" href="/course/details/{{course._id}}/page/0"><i class="far fa-clock"></i></a>
  </div>
  <div class="circular-container">
    <a id="door"href="/course/details/{{course._id}}/page/1"><i class="fas fa-door-open"></i></a>
  </div>

  <div class="circular-container">
    <a id="gavel"href="/course/details/{{course._id}}/page/2"><i class="fas fa-gavel"></i></a>
  </div>
  <div class="circular-container">
    <a id="map" href="/course/details/{{course._id}}/page/3"><i class="fas fa-sitemap"></i></a>
    
  </div>
  <div class="circular-container">
    <a id="task"href="/course/details/{{course._id}}/page/4"><i class="fas fa-tasks"></i></a>
  </div>
  <div class="circular-container">
    <a id="file" href="/course/details/{{course._id}}/page/5"><i class="far fa-file"></i></a>
  </div>
  <div class="circular-container">
    <a id="video" href="/course/details/{{course._id}}/page/6"><i class="fas fa-video"></i></a>
  </div>
</div>

